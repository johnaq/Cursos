{{>navbar}}

<table class="table">
  <thead class="thead-dark">
    <tr>
      <th scope="col">Nombre</th>
      <th scope="col">Descripcion</th>
      <th scope="col">Valor</th>
      <th scope="col">Opciones</th>
    </tr>
  </thead>
  <tbody>
    {{#each inscripcion}}
    <tr>
      <td scope="row">{{nombreCurso}}</td>
      <td>{{descripcion}}</td>
      <td>{{valor}}</td>
      <td><a class="btn btn-danger" href="/inscripciones/eliminar/{{id}}" role="button">Eliminar</a></td>
    </tr>
    {{/each}}
</table>