{{>navbar}}

<table class="table">
  <thead class="thead-dark">
    <tr>
      <th scope="col">Nombre</th>
      <th scope="col">Descripcion</th>
      <th scope="col">Valor</th>
      <th scope="col">Opciones</th>
    </tr>
  </thead>
  <tbody>
    
    {{#each inscripciones}}
    <tr>
      <td scope="row">{{idCurso.nombreCurso}}</td>
      <td>{{idCurso.descripcion}}</td>
      <td>{{idCurso.valor}}</td>
      <td><a class="btn btn-danger" href="/inscripciones/eliminar/{{_id}}" role="button">Eliminar</a></td>
    </tr>
    {{/each}}
</table>